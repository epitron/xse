INSTPGMFLAGS = -m 0755

all:: ad2c
depend:
	@echo 'no need to depend for ad2c'
ad2c:
	@echo 'ad2c is up to date'

install:: ad2c.script
	$(INSTALL) -c $(INSTPGMFLAGS) ad2c.script $(BINDIR)/ad2c

install.man:: ad2c.man
	$(INSTALL) -c $(INSTMANFLAGS) ad2c.man $(MANDIR)/ad2c.1

